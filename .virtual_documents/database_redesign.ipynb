        # Link receipt to created invoice
        cursor.execute("""
            UPDATE receipt_invoices 
            SET created_invoice_id = %s, verification_status = 'verified', 
                verified_by = %s, verified_at = NOW()
            WHERE receipt_invoice_id = %s
